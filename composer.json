{
  "name": "userhub/sdk",
  "description": "UserHub PHP SDK",
  "keywords": ["api", "sdk", "userhub"],
  "homepage": "https://userhub.com",
  "license": "MIT",
  "require": {
    "php": ">=8.1.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "donatj/mock-webserver": "^2.7",
    "friendsofphp/php-cs-fixer": "3.65",
    "phpstan/phpstan": "^2.0.3",
    "phpunit/phpunit": "^11"
  },
  "autoload": {
    "psr-4": {
      "UserHub\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UserHub\\": [
        "tests/"
      ]
    }
  }
}
